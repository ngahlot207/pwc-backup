                            <apex:repeat value="{!splittedIFSC}" var="ifscOrMicr">
                                <td class="center" style="width:3%;font-size:9px;">{!ifscOrMicr}</td>
                            </apex:repeat>
                            
                        </tr>                         
                    </table>
                </td>
            </tr>
        </table>
        <table  id="amount_Of_Inr" class="mandate-info-align" style="margin-top: 2px; margin-right:5px;">
