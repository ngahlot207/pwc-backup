                        <td class="noBorder" style="width:16%;text-align:center;font-size:9px;">Bank a/c number</td>
                        <apex:repeat value="{!splittedAccNumInDigits}" var="accNum">
                            <td class="center" style = "width:2.5%;height:0.5px;font-size:9px;">{!accNum}</td>
                        </apex:repeat>
                    </tr> 
                    </table>
                </td>
            </tr>
