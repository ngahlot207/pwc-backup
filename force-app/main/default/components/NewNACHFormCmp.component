            </apex:outputPanel>
            <apex:outputPanel rendered="{!!isHDFCBank}">
                <span style="padding-right:4px;font-size:9px;"><img src="{!$Resource.CheckB_Img}" style="height:10px;width:12px;margin-bottom:-2px;"/>&nbsp;Monthly</span>
